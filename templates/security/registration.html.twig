{% extends 'base.html.twig' %}
{% block title %}SCU - Inscription{% endblock %}

{% block body %}
    {{ form_start(form) }}

    {{ form_row(form.login, {'label': 'Nom d\'utilisateur'}) }}
    {{ form_row(form.email, {'label': 'Adresse E-mail'}) }}
    {{ form_row(form.password, {'label': 'Mot de passe'}) }}
    {{ form_row(form.confirm_password, {'label': 'Confirmation du Mot de passe'}) }}
    <button type="submit" class = "btn btn-success">Inscription !</button>
    {{ form_end(form) }}
{% endblock %}